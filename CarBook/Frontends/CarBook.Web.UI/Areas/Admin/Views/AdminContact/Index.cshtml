@model List<CarBook.Dto.ContactDtos.ResultContactDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
    int count = 0;
}
<link href="~/Admin/vertical/assets/plugins/datatables/dataTables.bootstrap4.css" rel="stylesheet" type="text/css" />
<link href="~/Admin/vertical/assets/plugins/datatables/responsive.bootstrap4.css" rel="stylesheet" type="text/css" />
<link href="~/Admin/vertical/assets/plugins/datatables/buttons.bootstrap4.css" rel="stylesheet" type="text/css" />
<link href="~/Admin/vertical/assets/plugins/datatables/select.bootstrap4.css" rel="stylesheet" type="text/css" />
<div class="page-content">
    <div class="container-fluid">

        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Basic Data Table</h4>
                        <p class="card-subtitle mb-4">
                            DataTables has most features enabled by default, so all you need to do to use it with your own tables is to call the construction
                            function:
                            <code>$().DataTable();</code>.
                        </p>

                        <table id="basic-datatable" class="table dt-responsive nowrap">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Subject</th>
                                    <th>Message</th>
                                    <th>Email</th>
                                    <th>SendDate</th>
                                </tr>
                            </thead>


                            <tbody>
                                @foreach (var item in Model)
                                {


                                    <tr>
                                        <td>@item.Name</td>
                                        <td>@item.Subject</td>
                                        <td>@item.Message</td>
                                        <td>@item.Email</td>
                                        <td>@item.SendDate</td>

                                    </tr>
                                }
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </div>

    </div>
</div>



